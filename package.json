{
  "name": "@gicm/opus67",
  "version": "6.3.0",
  "description": "OPUS 67 v6.3.0 'Context Engineering' - 141 Skills • 82 MCPs • 10 Modes • 107 Agents - Multi-stage retrieval, hierarchical memory, OpenSkills compatible",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "opus67": "./dist/unified-boot.js",
    "opus67-server": "./dist/brain/server.js",
    "opus67-mcp": "./dist/mcp-server.js",
    "opus67-legacy": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/brain/server.js",
    "test": "vitest",
    "test:benchmark": "vitest run src/tests/benchmark.test.ts",
    "test:llm": "vitest run src/tests/llm-benchmark/llm-benchmark.test.ts",
    "benchmark": "node --loader ts-node/esm src/tests/llm-benchmark/runner.ts",
    "benchmark:quick": "node --loader ts-node/esm src/tests/llm-benchmark/runner.ts --quick",
    "benchmark:full": "node --loader ts-node/esm src/tests/llm-benchmark/runner.ts --full",
    "benchmark:memory": "npx tsx src/benchmark/run-memory.ts",
    "bootstrap:memory": "npx tsx src/memory/unified/bootstrap.ts",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "ai",
    "llm",
    "mcp",
    "solana",
    "gicm",
    "opus67"
  ],
  "author": "gICM",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "fastify": "^4.28.0",
    "@fastify/websocket": "^10.0.0",
    "@fastify/cors": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "yaml": "^2.3.4",
    "eventemitter3": "^5.0.1",
    "zod": "^3.23.8",
    "glob": "^10.3.10"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0",
    "eslint": "^8.56.0"
  },
  "files": [
    "dist",
    "docs",
    "skills",
    "mcp",
    "modes",
    "config",
    "scripts",
    "registry",
    "docker",
    "agents",
    "THE_DOOR.md",
    ".env.example"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/0motionguy/gicm-opus67.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
