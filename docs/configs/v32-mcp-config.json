{
  "version": "3.2.0",
  "codename": "The Solana Stack",
  "releaseDate": "2025-12-02",
  "description": "Solana-native AI development system with universal infrastructure",
  
  "mcpServers": {
    "tier1_universal": {
      "github": {
        "source": "github.com/github/github-mcp-server",
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-github@latest"],
        "env": {
          "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PAT}"
        },
        "tools": [
          "search_repositories",
          "get_file_contents",
          "create_issue",
          "update_issue",
          "create_pull_request",
          "list_commits",
          "search_code",
          "create_branch",
          "push_files"
        ],
        "cost": "FREE",
        "effort": "low",
        "docs": "https://github.com/github/github-mcp-server"
      },
      
      "mem0": {
        "source": "github.com/coleam00/mcp-mem0",
        "command": "docker",
        "args": ["run", "--rm", "-i", "-e", "TRANSPORT", "-e", "LLM_PROVIDER", "-e", "LLM_API_KEY", "-e", "DATABASE_URL", "mcp/mem0"],
        "env": {
          "TRANSPORT": "stdio",
          "LLM_PROVIDER": "openai",
          "LLM_API_KEY": "${OPENAI_API_KEY}",
          "DATABASE_URL": "${POSTGRES_URL}"
        },
        "tools": [
          "save_memory",
          "get_all_memories",
          "search_memories",
          "delete_memory"
        ],
        "cost": "FREE (self-hosted) or $20/mo cloud",
        "effort": "medium",
        "docs": "https://github.com/coleam00/mcp-mem0"
      },
      
      "qdrant": {
        "source": "github.com/qdrant/mcp-server-qdrant",
        "command": "uvx",
        "args": ["mcp-server-qdrant"],
        "env": {
          "QDRANT_URL": "http://localhost:6333",
          "COLLECTION_NAME": "opus67",
          "EMBEDDING_MODEL": "sentence-transformers/all-MiniLM-L6-v2"
        },
        "tools": [
          "qdrant-store",
          "qdrant-find"
        ],
        "cost": "FREE (self-hosted)",
        "effort": "medium",
        "docs": "https://github.com/qdrant/mcp-server-qdrant"
      },
      
      "supabase": {
        "source": "github.com/supabase-community/supabase-mcp",
        "command": "npx",
        "args": ["-y", "@supabase/mcp-server"],
        "env": {
          "SUPABASE_ACCESS_TOKEN": "${SUPABASE_PAT}"
        },
        "tools": [
          "list_projects",
          "execute_sql",
          "apply_migration",
          "list_tables",
          "get_logs",
          "generate_typescript_types",
          "create_project",
          "create_branch"
        ],
        "cost": "FREE (Supabase free tier)",
        "effort": "low",
        "docs": "https://supabase.com/docs/guides/getting-started/mcp"
      },
      
      "docker": {
        "source": "github.com/QuantGeekDev/docker-mcp",
        "command": "uvx",
        "args": ["docker-mcp"],
        "tools": [
          "create_container",
          "list_containers",
          "get_logs",
          "deploy_compose",
          "stop_container"
        ],
        "cost": "FREE",
        "effort": "low",
        "docs": "https://github.com/QuantGeekDev/docker-mcp"
      }
    },
    
    "tier2_solana": {
      "jupiter": {
        "source": "github.com/kukapay/jupiter-mcp",
        "command": "node",
        "args": ["./mcp/jupiter-mcp/server/index.js"],
        "env": {
          "SOLANA_RPC_URL": "${HELIUS_RPC_URL}",
          "PRIVATE_KEY": "${SOLANA_PRIVATE_KEY}"
        },
        "tools": [
          "get_swap_quote",
          "execute_swap",
          "get_token_info"
        ],
        "cost": "FREE (network fees only)",
        "effort": "medium",
        "docs": "https://github.com/kukapay/jupiter-mcp"
      },
      
      "solana": {
        "source": "github.com/sendaifun/solana-mcp",
        "command": "npx",
        "args": ["solana-mcp"],
        "env": {
          "RPC_URL": "${HELIUS_RPC_URL}",
          "SOLANA_PRIVATE_KEY": "${SOLANA_PRIVATE_KEY}"
        },
        "tools": [
          "get_balance",
          "get_token_accounts",
          "get_transaction",
          "get_account_info",
          "get_recent_blockhash",
          "send_transaction"
        ],
        "cost": "FREE (RPC costs)",
        "effort": "low",
        "docs": "https://github.com/sendaifun/solana-mcp"
      },
      
      "solana-web3": {
        "source": "github.com/FrankGenGo/solana-web3js-mcp-server",
        "command": "node",
        "args": ["./mcp/solana-web3js/dist/index.js"],
        "tools": [
          "generate_keypair",
          "deploy_program",
          "create_token",
          "mint_tokens",
          "transfer"
        ],
        "cost": "FREE (network fees)",
        "effort": "medium",
        "docs": "https://github.com/FrankGenGo/solana-web3js-mcp-server"
      },
      
      "anchor": {
        "type": "custom_build",
        "description": "Parse Anchor IDLs and generate instructions from natural language",
        "command": "node",
        "args": ["./mcp/anchor-mcp/index.js"],
        "env": {
          "IDL_PATH": "./target/idl/",
          "PROGRAM_ID": "${ICM_PROGRAM_ID}",
          "RPC_URL": "${HELIUS_RPC_URL}"
        },
        "tools": [
          "parse_idl",
          "generate_instruction",
          "execute_instruction",
          "get_account_data",
          "simulate_transaction"
        ],
        "cost": "FREE (build yourself)",
        "effort": "high",
        "unique": true,
        "note": "KILLER FEATURE - No competitor has this"
      },
      
      "chainstack-solana": {
        "source": "Chainstack MCP",
        "command": "npx",
        "args": ["-y", "@chainstacklabs/solana-mcp"],
        "env": {
          "CHAINSTACK_API_KEY": "${CHAINSTACK_KEY}",
          "SOLANA_ENDPOINT": "${CHAINSTACK_SOLANA_URL}"
        },
        "tools": [
          "get_signatures",
          "get_token_largest_accounts",
          "get_program_accounts",
          "subscribe_logs"
        ],
        "cost": "$29/mo (Growth tier)",
        "effort": "low",
        "docs": "https://chainstack.com/mcp-for-web3-builders-solana-evm-and-documentation-server-by-chainstack/"
      }
    },
    
    "tier3_devops": {
      "notion": {
        "source": "github.com/makenotion/notion-mcp-server",
        "command": "npx",
        "args": ["-y", "@notionhq/notion-mcp-server"],
        "env": {
          "OPENAPI_MCP_HEADERS": "{\"Authorization\": \"Bearer ${NOTION_TOKEN}\", \"Notion-Version\": \"2022-06-28\"}"
        },
        "tools": [
          "search",
          "create_page",
          "update_page",
          "query_database",
          "create_database"
        ],
        "cost": "FREE",
        "effort": "low",
        "docs": "https://developers.notion.com/docs/mcp"
      },
      
      "sentry": {
        "source": "Sentry Official",
        "type": "http",
        "url": "https://mcp.sentry.dev/mcp",
        "tools": [
          "list_projects",
          "list_issues",
          "get_issue",
          "list_events",
          "trigger_seer"
        ],
        "cost": "FREE (free tier)",
        "effort": "very_low",
        "docs": "https://docs.sentry.io/product/sentry-mcp/"
      }
    }
  },
  
