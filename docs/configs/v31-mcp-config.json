  "mcpServers": {
    "firecrawl": {
      "description": "Production-grade web scraping and crawling",
      "command": "npx",
      "args": ["-y", "firecrawl-mcp"],
      "env": { "FIRECRAWL_API_KEY": "${FIRECRAWL_API_KEY}" },
      "tools": ["scrape", "crawl", "map", "extract"],
      "cost": "$16/month or self-host",
      "docs": "https://docs.firecrawl.dev"
    },
    
    "tavily": {
      "description": "AI-optimized web search for agents",
      "command": "npx",
      "args": ["-y", "mcp-remote", "https://mcp.tavily.com/mcp/?tavilyApiKey=${TAVILY_API_KEY}"],
      "tools": ["tavily-search", "tavily-extract", "tavily-news", "tavily-crawl"],
      "cost": "1000 free/month, then $0.01/search",
      "docs": "https://docs.tavily.com/documentation/mcp"
    },
    
    "exa": {
      "description": "Semantic search + code context + company research",
      "command": "npx",
      "args": ["-y", "exa-mcp-server", "tools=web_search_exa,get_code_context_exa,deep_search_exa,company_research_exa,linkedin_search_exa,crawling_exa"],
      "env": { "EXA_API_KEY": "${EXA_API_KEY}" },
      "tools": ["web_search_exa", "deep_search_exa", "get_code_context_exa", "company_research_exa", "linkedin_search_exa", "crawling_exa"],
      "cost": "Free tier + usage-based",
      "docs": "https://docs.exa.ai/reference/exa-mcp"
    },
    
    "playwright": {
      "description": "Official Microsoft browser automation MCP",
      "command": "npx",
      "args": ["@playwright/mcp@latest", "--headless"],
      "tools": ["browser_navigate", "browser_click", "browser_type", "browser_screenshot", "browser_pdf", "browser_execute_js", "browser_wait"],
      "cost": "FREE",
      "docs": "https://github.com/microsoft/playwright-mcp"
    },
    
    "stagehand": {
      "description": "AI browser framework with act/extract/observe",
      "command": "npx",
      "args": ["-y", "@browserbasehq/stagehand-mcp"],
      "tools": ["act", "extract", "observe", "agent_execute", "goto", "screenshot"],
      "cost": "FREE local, Browserbase cloud pricing",
      "docs": "https://docs.stagehand.dev"
    },
    
    "context7": {
      "description": "Up-to-date library documentation injection",
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"],
      "tools": ["resolve-library-id", "get-library-docs"],
      "cost": "FREE",
      "docs": "https://github.com/upstash/context7"
    },
    
    "screenpipe": {
      "description": "24/7 desktop recording with searchable history",
      "command": "uv",
      "args": ["run", "screenpipe-mcp"],
      "tools": ["search_screen", "search_audio", "get_timeline", "search_ocr", "get_context"],
      "cost": "FREE (local)",
      "docs": "https://docs.screenpi.pe/mcp-server"
    },
    
    "jina": {
      "description": "FREE URL to markdown conversion",
      "type": "http",
      "baseUrl": "https://r.jina.ai",
      "tools": ["read_url", "search_web", "read_pdf"],
      "cost": "FREE",
      "docs": "https://jina.ai/reader"
    },
    
    "brightdata": {
      "description": "Enterprise-grade web data infrastructure",
      "command": "npx",
      "args": ["-y", "@anthropic/brightdata-mcp"],
      "tools": ["serp_search", "web_scraper", "proxy_rotate", "dataset_download"],
      "cost": "Enterprise pricing",
      "docs": "https://brightdata.com"
    },
    
    "browserbase": {
      "description": "Cloud browser infrastructure",
      "command": "npx",
      "args": ["-y", "@browserbasehq/mcp"],
      "env": { "BROWSERBASE_API_KEY": "${BROWSERBASE_API_KEY}" },
      "tools": ["create_session", "control_browser", "live_view", "session_replay"],
      "cost": "Usage-based",
      "docs": "https://browserbase.com"
    }
  },

